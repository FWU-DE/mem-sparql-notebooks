  },
  {
    "kind": 2,
    "language": "sparql",
    "value": "PREFIX lp: <https://w3id.org/lehrplan/ontology/>\n\nSELECT *\n\nFROM <http://mem.org/rlp>\nWHERE { \n  <http://localhost:8080/resource/128> rdf:type lp:LP_0000433 ;\n    lp:LP_0000008* ?teil .\n  }  ",
    "metadata": {}
